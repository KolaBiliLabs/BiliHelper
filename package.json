{
  "name": "KolaBiliHelper",
  "productName": "KolaBiliHelper",
  "version": "0.1.0",
  "description": "An Electron-Vue application to help manage Bilibili music/video favorites.",
  "author": "imddc",
  "github": "https://github.com/KolaBiliLabs/BiliHelper",
  "license": "MIT",
  "repository": "github:KolaBiliLabs/BiliHelper",
  "licenses-file": "LICENSE",
  "main": "./out/main/index.js",
  "scripts": {
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@formkit/auto-animate": "^0.8.2",
    "@tailwindcss/vite": "^4.1.10",
    "@types/electron": "^1.6.12",
    "@vueuse/core": "^13.5.0",
    "axios": "^1.10.0",
    "better-sqlite3": "^9.4.3",
    "dayjs": "^1.11.13",
    "electron-log": "^5.4.1",
    "electron-updater": "^6.3.9",
    "epic-spinners": "^2.0.0",
    "express": "^5.1.0",
    "fastify": "^5.3.3",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^11.2.0",
    "howler": "^2.2.4",
    "lodash-es": "^4.17.21",
    "lucide-vue-next": "^0.517.0",
    "md5": "^2.3.0",
    "music-metadata": "^8.0.0",
    "nanoid": "^5.1.5",
    "node-fetch": "^3.3.2",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.3.0",
    "qs": "^6.14.0",
    "socket.io": "^4.8.1",
    "tailwindcss": "^4.1.10",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.14.1",
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/better-sqlite3": "^7.6.9",
    "@types/express": "^5.0.3",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/fs-extra": "^11.0.4",
    "@types/howler": "^2.2.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "electron": "^35.1.5",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-vue": "^10.0.0",
    "naive-ui": "^2.42.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "sass": "^1.89.2",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "electron",
      "esbuild"
    ]
  }
}
