{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@constants/*": [
        "constants/*"
      ]
    }
  },
  "include": [
    "src/env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "preload/*.d.ts",
    "constants/**/*"
  ]
}
